    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-mkcert": "^1.9.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^1.0.9"
